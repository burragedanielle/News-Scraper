{
  "name": "news-scraper",
  "version": "1.0.0",
  "description": "News Scraper Project React App KU Bootcamp",
  "main": "server.js",
  "scripts": {
    "client": "npm run start --prefix client",
    "server": "nodemon index.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node index.js",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/burragedanielle/News-Scraper.git"
  },
  "author": "Danielle Burrage",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/burragedanielle/News-Scraper/issues"
  },
  "homepage": "https://github.com/burragedanielle/News-Scraper#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^5.1.0",
    "express": "^4.17.1",
    "mongodb": "^3.5.3",
    "mongoose": "^5.9.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}